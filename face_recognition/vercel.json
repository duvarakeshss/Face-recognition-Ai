{
    "version": 2,
    "buildCommand": "cd face_recognition && ./vercel-install.sh",
    "builds": [
      {
        "src": "app_light.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app_light.py"
      }
    ],
    "env": {
      "PYTHONPATH": "."
    }
  }